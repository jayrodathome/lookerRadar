  {
    "dataStudio": {
      "name": "Radar Chart (Chart.js)",
      "logoUrl": "https://jayrodathome.github.io/lookerRadar/logo.png",
      "company": "Jarrod Lilly",
      "companyUrl": "https://www.netechair.com",
      "addonUrl": "https://jayrodathome.github.io/lookerRadar/index.html",
      "supportUrl": "https://github.com/jayrodathome/lookerRadar",
      "description": "Multi-dimensional radar chart built with Chart.js",
      "authModes": ["ANONYMOUS"],
      "types": ["CHART"],
      "style": [
        {
          "type": "COLOR",
          "name": "Series Colors",
          "id": "seriesColors",
          "defaultValue": [
            { "color": "#D33E47" },
            { "color": "#AAAAAA" }
          ]
        },
        {
          "type": "NUMBER",
          "name": "Line Width",
          "id": "lineWidth",
          "defaultValue": 2,
          "min": 1,
          "max": 10
        },
        {
          "type": "BOOLEAN",
          "name": "Fill Areas",
          "id": "fillAreas",
          "defaultValue": true
        }
      ]
    }
  }
